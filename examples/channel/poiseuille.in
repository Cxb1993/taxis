grid
{
   type   gmsh 
   file   channel.msh
   cell   median
}

numeric
{
   time_mode    steady
   time_scheme  lusgs
   cfl          1e6
   max_iter     50000
   final_time   1e20
   min_residue  1.0e-10
   reconstruct  second
   bc_scheme    weak
}

material
{
   gamma       1.4
   gas_const   287
   viscosity   constant  1.12
   prandtl     0.72
   model       ns
   flux        roe
}

constants
{
   L     5.0
   T     350.0
   pin   1.05e5
   pout  1.0e5
   Uc    4.46428571428571e+02
}

initial_condition
{
   temperature  T
   xvelocity    0.0
   yvelocity    0.0
   zvelocity    0.0
   pressure     pin + (x/L)*(pout - pin)
}

boundary
{
   100001 // inlet boundary
   {
      type         farfield
      temperature  T
      xvelocity    Uc*y*(1 - y)
      yvelocity    0.0
      zvelocity    0.0
      pressure     pin
   }

   100002 // outlet boundary
   {
      type       pressure
      pressure   pout
   }

   100003
   100004
   {
      type        noslip
      xvelocity   0.0
      yvelocity   0.0
      zvelocity   0.0
   }
}

integrals
{
}

output
{
   format      vtk
   frequency   1000
   variables
   {
      mach density
   }
   restart     true
}
