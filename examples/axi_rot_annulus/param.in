grid
{
   type   gmsh 
   file   annulus.msh
   cell   median
}

numeric
{
   time_mode    steady
   time_scheme  lusgs
   time_step    0.0
   cfl          1e6
   max_iter     500000
   final_time   1.0e20
   min_residue  1.0e-6
   reconstruct  second
   bc_scheme    strong
}

material
{
   gamma       1.4
   gas_const   287.0
   viscosity   constant  0.01
   prandtl     0.72
   model       ns
   flux        roe
}

constants
{
   T    300.0
   p    86100.0
   omg  100.0
}

initial_condition
{
   temperature   T
   xvelocity     0.0
   yvelocity     0.0
   zvelocity     omg*x
   pressure      p
}

boundary
{
   100001
   100002
   100003
   100004
   {
      type         noslip
      xvelocity    0.0
      yvelocity    0.0
      zvelocity    omg*x
      temperature  T
   }
}

integrals
{
}

output
{
   format     vtk
   frequency  1000
   variables
   {
      mach  
      density
   }
   restart    false
   global_KE  false
}
