CXX=CC 
longlong = -ext -DLONGLONG
O = -DNDEBUG   $(longlong) +Onolibcalls  +p +DA1.1 +O3 #  $(longlong) +Onolibcalls  +p +DA1.1
G = -g  $(longlong) +Onolibcalls  +p +d +DA1.1
CXXFLAGS= $(O) 
CXXGRAPHIC =   -I/usr/include/X11R6  -DDRAWING   -DDEBUG 
CXXLINKFLAG =  -Wl,+FPVZOuiD
X11LIB = -L/usr/lib/X11R6  -lX11
X11INCLUDE= -I/usr/include/X11R6
STDLIB = -lm 
F77 = f77
F77FLAGS = +U77 +DA1.1
